{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "endOfLine": "lf",
  "useTabs": false,
  "quoteProps": "as-needed",
  "arrowParens": "always",
  "jsxBracketSameLine": false,
  "tailwindConfig": "./tailwind.config.js",
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
